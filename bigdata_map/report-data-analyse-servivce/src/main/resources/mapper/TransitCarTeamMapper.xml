    <select id="selectCountCarTeam" parameterType="com.edgedo.reportdata.queryvo.TransitCarTeamQuery" resultType="int">
        /*!mycat:sql=select app_id from zhyg_db where app_id='${cityId}'*/ select count(1) from transit_car_team
        <trim prefix="WHERE (" suffix=")" prefixOverrides="AND | OR">
            <if test="teamName != null and teamName != ''">
                AND TEAM_NAME LIKE concat('%',#{teamName,jdbcType = VARCHAR},'%')
            </if>
            <if test="xianquId != null and xianquId != ''">
                AND XIANQU_ID = #{xianquId,jdbcType = VARCHAR}
            </if>
            <if test="companyLavel != null and companyLavel != ''">
                AND COMPANY_LAVEL = #{companyLavel,jdbcType = VARCHAR}
            </if>
        </trim>
    </select>

    <select id="selectCarTeamInfoListPage" parameterType="com.edgedo.reportdata.queryvo.TransitCarTeamQuery" resultMap="transitCarTeamMap">
        /*!mycat:sql = select app_id from zhyg_db where app_id = '${cityId}'*/
        select <include refid="Base_Column_List"/> from transit_car_team
        <trim prefix="WHERE (" suffix=")" prefixOverrides="AND | OR">
            <if test="teamName != null and teamName != ''">
              AND TEAM_NAME LIKE concat('%',#{teamName},'%')
            </if>
            <if test="teamId != null and teamId != ''">
                AND ID = #{teamId}
            </if>
            <if test="xianquId != null and xianquId != ''">
              AND XIANQU_ID = #{xianquId}
            </if>
            <if test="companyLavel != null and companyLavel != ''">
                AND COMPANY_LAVEL = #{companyLavel}
            </if>
        </trim>
    </select>

    <select id="selectCarTeamInfo" parameterType="com.edgedo.reportdata.queryvo.TransitCarTeamQuery" resultMap="transitCarTeamMap">
        /*!mycat:sql = select app_id from zhyg_db where app_id = '${cityId}'*/
        select <include refid="Base_Column_List"/> from transit_car_team WHERE DATA_STATE = '1'
        <if test="companyLavel != null and companyLavel != ''">
            AND COMPANY_LAVEL = #{companyLavel}
        </if>
        <if test="teamId != null and teamId != ''">
            AND ID = #{teamId}
        </if>
    </select>

