});

$(function(){
    var queryArgs = getQueryParam();
    var param = "cityId=130300";
    var teamName = decodeURI(queryArgs.teamName);
    var teamId = decodeURI(queryArgs.teamId);
    if(teamId != "undefined" && teamId != null && teamId != ''){
        param += "&teamId=" + teamId;
    }
    if(teamName != "undefined" && teamName != null && teamName != ''){
        param += "&teamName=" + teamName;
    }
    var companlavel = decodeURI(queryArgs.companlavel);
    if(companlavel != "undefined" && companlavel != null && companlavel != ''){
        param += "&companlavel=" + companlavel;
    }
    var xianquId = decodeURI(queryArgs.xianquId);
    if(xianquId != "undefined" && xianquId != null && xianquId != ''){
        param += "&xianquId=" + xianquId;
    }

    //查询企业相关信息
    searchCarTeamInfo(param);
})

    function searchCarTeamInfo(param){
        $.ajax({
            type:'POST',
            url: basePathUrl + '/admin/carTeam/searchTransitTeamInfoByQuery.jsn',
            data:param,
            dataType:'json',
            beforeSend: function (XMLHttpRequest) {
                XMLHttpRequest.setRequestHeader("access-token", getStorage("access_token"));
            },
            success:function(data){
                debugger;
                if(data.success){
                    var teamInfo = data.list[0];
                    $("#area").html("所属区域："+teamInfo.provinceName+">"+teamInfo.cityName+">"+teamInfo.xianquName);
                    $("#teamName").html(teamInfo.teamName);
                    $("#jyCertNumber").html(teamInfo.jyCertNumber);
                    $("#companyLevel").html(checkTeamType(teamInfo.companyLevel));
                    $("#licenseValidDate").html(dateFormat("yyyy-MM-dd",new Date(teamInfo.licenseValidDate)));
                    $("#economicType").html(teamInfo.economicType);
                    $("#businessCertCode").html(teamInfo.businessCertCode);
                    $("#legalRepresentName").html(teamInfo.legalRepresentName);
                    $("#jyManName").html(teamInfo.jyManName);
                    $("#legalRepresentLicenceType").html(teamInfo.legalRepresentLicenceType);
                    $("#jyChargeManPhone").html(teamInfo.jyChargeManPhone);
                    $("#teamType").html(teamInfo.teamType);
                    $("#teamAdminName").html(teamInfo.teamAdminName);
                    $("#teamAddress").html(teamInfo.teamAddress);
                    $("#teamAdminPhone").html(teamInfo.teamAdminPhone);
                    if(teamInfo.checkRenzhengFlag == 1){
                        $("#checkRenzhengFlag").html("是");
                    }else{
                        $("#checkRenzhengFlag").html("否");
                    }
                    $("#checkPhoneFlag").html(teamInfo.checkPhoneFlag);
                    $("#carAmount").html(teamInfo.carAmount);
                    $("#gcAmount").html(teamInfo.gcAmount);
                    $("#administrator").html(teamInfo.administrator);
                    if(teamInfo.carSumTon != null && teamInfo.carSumTon != ''){
                        $("#carSumTon").html(teamInfo.carSumTon+"吨");
                    }else{
                        $("#carSumTon").html(teamInfo.carSumTon);
                    }
                    $("#postCode").html(teamInfo.postCode);
                    $("#teamEmail").html(teamInfo.teamEmail);
                    $("#remark").html(teamInfo.remark);
                    $("#jyCertImageSrc").attr("src",teamInfo.jyCertImageSrc);
                    $("#businessCertImageSrc").attr("src",teamInfo.businessCertImageSrc);
                    $("#jyManNameBookImageSrc").attr("src",teamInfo.jyManNameBookImageSrc);
                    $("#qrCodeImageSrc").attr("src",teamInfo.qrCodeImageSrc);
                    $("#businessCertImage").attr("src",teamInfo.businessCertImage);

                    selectTeamZhiDuList(teamInfo.id,teamInfo.companyLevel);

                    if(teamInfo.companyLevel == "TEAM_WXHWYS"){
                        var html = "";
                        if(teamInfo.biaozhunLavel != null){
                            html +="<tr><td>标准化等级：</td><td>"+teamInfo.biaozhunLavel+"</td>";
                        }else{
                            html +="<tr><td>标准化等级：</td><td></td>";
                        }
                        if(teamInfo.zhengshuEndDate != null){
                            html += "<td>证书有效期至：</td><td>"+teamInfo.zhengshuEndDate+"</td></tr>";
                        }else{
                            html += "<td>证书有效期至：</td><td></td></tr>";
                        }
                        if(teamInfo.biaozhunPhoto != null){
                            html +="<tr><td>标准化证书图片：</td><td><img  style='width: 150px;' src='"+teamInfo.biaozhunPhoto+"'></td>";
                        }else{
                            html +="<tr><td>标准化证书图片：</td><td></td>";
                        }
                        if(teamInfo.cdghPhoto != null){
                            html += "<td>场地规划图片：</td><td><img  style='width: 150px;' src='"+teamInfo.cdghPhoto+"'></td></tr>";
                        }else{
                            html += "<td>场地规划图片：</td><td></td></tr>";
                        }
                        if(teamInfo.xfsPhoto != null){
                            html +="<tr><td>消防沙图片：</td><td><img  style='width: 150px;' src='"+teamInfo.xfsPhoto+"'></td>";
                        }else{
                            html +="<tr><td>消防沙图片：</td><td></td>";
                        }
                        if(teamInfo.xfqiuPhoto != null){
                            html += "<td>消防锹图片：</td><td><img  style='width: 150px;' src='"+teamInfo.xfqiuPhoto+"'></td></tr>";
                        }else{
                            html += "<td>消防锹图片：</td><td></td></tr>";
                        }
                        if(teamInfo.mhqPhoto != null){
