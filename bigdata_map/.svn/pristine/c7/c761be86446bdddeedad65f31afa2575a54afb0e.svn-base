        debugger;
        function searchCarPlateNum(){
            var searchCarPlateNum = $("#searchCarPlateNum").val();
            if(searchCarPlateNum == ""){
                $("dl dd").show();
            }else{
                $("dl dd").each(function(){
                    var carPlateNum = $(this).attr("carPlateNum");
                    if(typeof(carPlateNum) != "undefined"){
