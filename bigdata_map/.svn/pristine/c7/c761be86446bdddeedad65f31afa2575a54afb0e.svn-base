                                html += "<td style='text-align: left'><img style='width:150px' src='"+zhiduList[i].zhiduPhoto+"'/></td>";
                                if((i+1) < zhiduList.length){
                                    html += "<td style='text-align: right'>"+zhiduList[i+1].zhiduName+"</td>";
                                    html += "<td style='text-align: left'><img style='width:150px' src='"+zhiduList[i+1].zhiduPhoto+"'/></td>";
                                    html += "</tr>";
                                }else{
                                    html += "<td></td><td></td></tr>";
                                }
                            }
                        }

                    }
                    $("#teamInfo").html($("#teamInfo").html() + html);
                }else {
                    if(data.code==1001){
                        location.href = '../../login.html';
                    }else {
                        layer.msg('后台异常', {
                            icon: 5
                        });
                    }
                }
            }
        })
    }
function goShouYe() {
    window.location.href = "shouye.html";
}
</script>





