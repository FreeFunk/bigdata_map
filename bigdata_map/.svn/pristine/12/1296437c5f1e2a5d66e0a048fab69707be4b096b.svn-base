  		<include refid="where"/>
  </select>

	<select id="loadByDriverIdAndYear" parameterType="com.edgedo.drawing.entity.BigdataDriverDrawInfo" resultMap="BaseResultViewMap">
		select <include refid="Base_Column_List"/> from bigdata_driver_draw_info
		WHERE YEAR_NUM = #{yearNum} and DRIVER_ID = #{driverId}
