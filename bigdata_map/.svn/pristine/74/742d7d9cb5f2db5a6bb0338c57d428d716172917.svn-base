    function change() {
        clearInterval(sh);
        sh=setInterval(changeOnlineNum,4000);
        clearInterval(ch);
        ch=setInterval(changeOperationNum,5000);
    }

    function changeOnlineNum(){
        var onlineNumVal  = $("#onlineNumVal").val();
        var s = parseInt(randomNum(-10,10));
        $("#onlineNum")[0].innerText = parseInt(onlineNumVal) + parseInt(s);
    }
    function changeOperationNum(){
        var operationNumVal  = $("#operationNumVal").val();
        var s = parseInt(randomNum(-10,10));
        $("#operationNum")[0].innerText = parseInt(operationNumVal) + parseInt(s);
    }
    //生成从minNum到maxNum的随机数
    function randomNum(minNum,maxNum){
        switch(arguments.length){
            case 1:
                return parseInt(Math.random()*minNum+1,10);
                break;
            case 2:
                return parseInt(Math.random()*(maxNum-minNum+1)+minNum,10);
                break;
            default:
                return 0;
                break;
        }
    }
    //切换地图点
    function changeMapPoint() {
        var iframe2 = document.getElementById('iframe2').contentWindow;
        iframe2.updateMapPoint();
        var iframe1 = document.getElementById('iframe1').contentWindow;
        iframe1.updateMapPoint();
    }

    var timeout;
    var options = {
        useEasing: false, 	    // 使用缓和
        useGrouping: false,      // 使用分组(是否显示千位分隔符,一般为 true)
        separator: ',',         // 分隔器(千位分隔符,默认为',')
        decimal: '.',           // 十进制(小数点符号,默认为 '.')
        prefix: '',	            // 字首(数字的前缀,根据需要可设为 $,¥,￥ 等)
        suffix: ''              // 后缀(数字的后缀 ,根据需要可设为 元,个,美元 等)
    };
    var countDemo;
    function alarmCountShow1(){
        var nowDate = new Date();
        var nowDate1 = nowDate.format("yyyy-MM-dd");
        if(nowDate1!='' && nowDate1==searchDate){
            $.ajax({
                type: "POST",
                url: basePathUrl+"/admin/bigdataAlarmRecord/alsrmCount.jsn",
                data: {'queryObj.alarmTime':searchDate,'queryObj.alarmType':'2'},
                dataType: "json",
                beforeSend: function (XMLHttpRequest) {
                    XMLHttpRequest.setRequestHeader("access-token", getStorage("access_token"));
                },
                success: function(data){
                    if(data.success){
                        if(data.data*4>$("#alarmRecordCount")[0].innerText){
                            countDemo = new CountUp('alarmRecordCount',  $("#alarmRecordCount")[0].innerText, data.data, 0, 30, options);
                            if (!countDemo.error) {
                                countDemo.start();
                            } else {
                                console.error(countDemo.error);
                            }
                        }
                        clearTimeout(timeout);
                        timeout = setTimeout(alarmCountShow1, 1000*30);
                        //target = 目标元素的 ID；//startVal = 开始值；//endVal = 结束值；//decimals = 小数位数，默认值是0；//duration = 动画延迟秒数，默认值是2；
                        $("#alarmRecordCount")[0].innerText=data.data;
                    }else {
                        /*layer.msg('后台异常', {
                            icon: 5
                        });*/
                    }
                }
            });
        }
    }

    function accMul(arg1,arg2)
    {
        var m=0,s1=arg1.toString(),s2=arg2.toString();
        try{m+=s1.split(".")[1].length}catch(e){}
        try{m+=s2.split(".")[1].length}catch(e){}
        return Number(s1.replace(".",""))*Number(s2.replace(".",""))/Math.pow(10,m)
    }


</script>

