                    debugger;
                    if(typeof(data.data) == "undefined"){

                    }else {
                        $("#fatigueRunNum").html(data.data.fatigueRunNum+"次");
                        $("#overspeedRunNum").html(data.data.overspeedRunNum+"次");
                        $("#teamSafetyNum").html(data.data.teamSafetyNum+"次");
                        $("#carOnlineRate").html(data.data.carOnlineRate+"%");
                        $("#carOperateRate").html(data.data.carOperateRate+"%");
                        $("#empAddNum").html(data.data.empAddNum+"次");
