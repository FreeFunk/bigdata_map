    //初始化日期
    function initLayDate(searchTime) {
        laydate.render({
            elem: '#searchDate',
            theme: '#884898',
            value: searchTime,
            btns: ['confirm'],
            done: function(value, date, endDate){
                if(value!=""){
                    var queryArgs = getQueryParam();
                    queryArgs.searchTime = value;
                    safetyProductionSituation(queryArgs);
                    safetyProductionEchartsData(queryArgs);
                }
            }
        });
    }
